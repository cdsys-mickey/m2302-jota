{
	"compilerOptions": {
		"target": "esnext",
		"jsx": "react",
		"module": "esnext",
		"moduleResolution": "node",
		"baseUrl": "src",
		"paths": {
			"@/*": [
				"*"
			],
			"@/modules/*": [
				"modules/*"
			]
		},
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}